{
  "[gotmpl]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features",
    "files.insertFinalNewline": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "editor.quickSuggestions": {
      "comments": "on",
      "other": "on",
      "strings": "on"
    },
    "files.trimTrailingWhitespace": false,
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "[scss]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "color-highlight.matchRgbWithNoFunction": true,
  "color-highlight.matchWords": true,
  "css.lint.hexColorLength": "ignore",
  "css.validate": false,
  "debug.onTaskErrors": "debugAnyway",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.accessibilitySupport": "off",
  "editor.autoClosingDelete": "always",
  "editor.codeActionsOnSave": {
    "source.fixAll": false,
    "source.fixAll.eslint": true,
    "source.organizeImports": false
  },
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorStyle": "underline",
  "editor.find.addExtraSpaceOnTop": false,
  "editor.fontFamily": "'JetBrains Mono', 'Fira Code', 'Droid Sans Mono', monospace",
  "editor.fontLigatures": true,
  "editor.fontWeight": "400",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.glyphMargin": true,
  "editor.guides.bracketPairs": "active",
  "editor.guides.bracketPairsHorizontal": true,
  "editor.inlineSuggest.enabled": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    80,
    100,
    120
  ],
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": false,
  "errorLens.enabledDiagnosticLevels": [
    "warning",
    "error"
  ],
  "errorLens.excludeBySource": [
    "cSpell",
    "Grammarly",
    "eslint"
  ],
  "errorLens.gutterIconSet": "circle",
  "errorLens.gutterIconsEnabled": true,
  "errorLens.messageEnabled": false,
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "eslint.quiet": true,
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "json5"
  ],
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "*.css": "$(capture).css.map, $(capture).*.css",
    "*.js": "$(capture).js.map, $(capture).*.js",
    "*.ts": "$(capture).js, $(capture).*.ts, $(capture).ts.map",
    ".env": "*.env, .env.*, .envrc, env.d.ts",
    ".gitignore": ".gitattributes, .gitmodules, .gitmessage, .mailmap, .git-blame*",
    "README.md": "SECURITY*, LICENSE*, CITATION.cff",
    "composer.json": ".php*.cache, composer.lock, phpunit.xml*, psalm*.xml",
    "dockerfile": ".dockerignore, docker-compose.*, dockerfile*",
    "go.mod": ".air*, go.sum",
    "hugo.log": ".hugo_build.lock, hugo_stats.json",
    "package.json": "package-lock.json, .fixpackrc, .editorconfig, .node-version, .npm*, .nvmrc, .versionrc*",
    "readme*": "authors, backers.md, changelog*, citation*, code_of_conduct.md, codeowners, contributing.md, contributors, copying, credits, governance.md, history.md, license*, maintainers, readme*, security.md, sponsors.md"
  },
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "**/layouts/**/*.*": "gohtml"
  },
  "files.autoSave": "onFocusChange",
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.simpleDialog.enable": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.mergeEditor": true,
  "git.untrackedChanges": "separate",
  "github.copilot.enable": {
    "*": true,
    "markdown": true,
    "plaintext": true,
    "yaml": true
  },
  "githubIssues.workingIssueFormatScm": "#${issueNumberLabel}",
  "githubPullRequests.fileListLayout": "tree",
  "go.lintOnSave": "workspace",
  "go.toolsManagement.autoUpdate": true,
  "less.validate": false,
  "peacock.affectStatusBar": false,
  "peacock.affectTitleBar": false,
  "peacock.showColorInStatusBar": false,
  "prettier.enable": false,
  "prettier.printWidth": 200,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "redhat.telemetry.enabled": false,
  "remote.SSH.configFile": "~/.ssh/config.d/servers.conf",
  "rpc.appName": "Visual Studio Code",
  "rpc.buttonEnabled": true,
  "rpc.checkIdle": false,
  "rpc.detailsIdling": "Cooking coffee",
  "rpc.idleText": ":coffee:",
  "rpc.idleTimeout": 1800,
  "scm.countBadge": "focused",
  "scm.diffDecorationsGutterWidth": 2,
  "scss.validate": false,
  "search.exclude": {
    "**/.git": true,
    "**/.github": true,
    "**/.nuxt": true,
    "**/.output": true,
    "**/.pnpm": true,
    "**/.vscode": true,
    "**/.yarn": true,
    "**/bower_components": true,
    "**/dist/**": true,
    "**/logs": true,
    "**/node_modules": true,
    "**/out/**": true,
    "**/package-lock.json": true,
    "**/pnpm-lock.yaml": true,
    "**/tmp": true,
    "**/yarn.lock": true
  },
  "stylelint.codeAction.disableRuleComment": {
    "location": "separateLine"
  },
  "stylelint.reportInvalidScopeDisables": true,
  "stylelint.reportNeedlessDisables": true,
  "stylelint.snippet": [
    "css",
    "less",
    "postcss",
    "scss",
    "sass"
  ],
  "stylelint.validate": [
    "css",
    "less",
    "postcss",
    "sass",
    "scss"
  ],
  "tailwindCSS.emmetCompletions": true,
  "terminal.integrated.automationProfile.linux": {
    "color": "terminal.ansiCyan",
    "icon": "server-environment",
    "name": "npm-server",
    "path": ""
  },
  "terminal.integrated.shellIntegration.enabled": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.defaultProfile.linux": "bash",
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.fontWeight": "300",
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.tabs.enableAnimation": false,
  "terminal.integrated.tabs.enabled": false,
  "tsErrorTranslator.showFullTranslation": true,
  "typescript.validate.enable": false,
  "window.autoDetectColorScheme": true,
  "window.commandCenter": true,
  "window.dialogStyle": "custom",
  "window.titleBarStyle": "custom",
  "workbench.activityBar.visible": true,
  "workbench.colorCustomizations": {
    "statusBar.background": "#1A1A1A",
    "statusBar.debuggingBackground": "#263238",
    "statusBar.noFolderBackground": "#212121",
    "editorStickyScroll.background": "#073642",
    "editorStickyScrollHover.background": "#002b36"
  },
  "workbench.colorTheme": "Default Light+",
  "workbench.editor.autoLockGroups": {
    "default": true
  },
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.enablePreview": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.limit.value": 5,
  "workbench.enableExperiments": false,
  "workbench.experimental.editor.dropIntoEditor.enabled": false,
  "workbench.iconTheme": null,
  "workbench.list.smoothScrolling": true,
  "workbench.preferredDarkColorTheme": "Solarized Dark",
  "workbench.preferredLightColorTheme": "Solarized Light",
  "workbench.startupEditor": "none",
  "workbench.tree.expandMode": "singleClick",
  "workbench.tree.indent": 10,
  "codesnap.realLineNumbers": true,
  "codesnap.containerPadding": "0.5em",
  "codesnap.backgroundColor": "#484848",
  "codesnap.boxShadow": "",
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "shellcheck.run": "onSave",
  "shellcheck.useWorkspaceRootAsCwd": true,
  "shellcheck.exclude": [
    "[\"1096\"]"
  ],
  "shellformat.flag": "-i >4",
  "todo-tree.general.statusBar": "total",
  "editor.codeLensFontFamily": "\"JetBrains Mono\", 'Fira Code', 'Droid Sans Mono', 'monospace', monospace",
  "editor.fontSize": 15,
  "editor.lineHeight": 1.4,
  "workbench.layoutControl.enabled": false,
  "terminal.integrated.shellIntegration.decorationIconError": "lightbulb",
  "editor.experimental.stickyScroll.enabled": true,
  "smarty.highlight": true,
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "breadcrumbs.symbolSortOrder": "name",
  "evenBetterToml.formatter.allowedBlankLines": 1,
  "evenBetterToml.formatter.columnWidth": 80,
  "evenBetterToml.schema.links": true,
  "grammarly.config.documentDialect": "british",
  "grammarly.config.documentDomain": "creative",
  "grammarly.config.suggestions.ConjunctionAtStartOfSentence": true,
  "grammarly.config.suggestions.InformalPronounsAcademic": true,
  "grammarly.config.suggestions.OxfordComma": true,
  "grammarly.config.suggestions.PrepositionAtTheEndOfSentence": true,
  "grammarly.config.suggestions.StylisticFragments": true,
  "grammarly.config.suggestions.UnnecessaryEllipses": true
}
