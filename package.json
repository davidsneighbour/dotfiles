{
  "name": "@davidsneighbour/dotfiles",
  "version": "1.0.3",
  "license": "GPL-3.0-or-later",
  "description": "David's Neighbour's .dotFiles",
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com>",
  "homepage": "https://github.com/davidsneighbour/dotfiles#readme",
  "repository": "github.com/davidsneighbour/dotfiles",
  "bugs": "https://github.com/davidsneighbour/dotfiles/issues",
  "dependencies": {
    "@dnb-org/remark-config": "^3.5.0",
    "@dnb-org/standard-version-config": "^3.5.0",
    "@dnb-org/textlint-config": "^3.5.0",
    "@themerdev/atom-syntax": "2.1.4",
    "@themerdev/atom-ui": "1.1.4",
    "@themerdev/chrome": "1.2.2",
    "@themerdev/colors-nova": "1.0.4",
    "@themerdev/css": "1.0.1",
    "@themerdev/firefox-addon": "^1.0.2",
    "@themerdev/firefox-color": "^1.0.3",
    "@themerdev/slack": "2.1.1",
    "@themerdev/sublime-text": "2.1.1",
    "@themerdev/vim": "3.3.1",
    "@themerdev/visual-studio": "^1.0.1",
    "@themerdev/vscode": "2.3.2",
    "@themerdev/wallpaper-block-wave": "4.0.3",
    "@themerdev/wallpaper-burst": "2.0.2",
    "@themerdev/wallpaper-circuits": "1.0.2",
    "@themerdev/wallpaper-diamonds": "2.0.2",
    "@themerdev/wallpaper-dot-grid": "1.0.1",
    "@themerdev/wallpaper-octagon": "4.0.2",
    "@themerdev/wallpaper-shirts": "2.0.2",
    "@themerdev/wallpaper-triangles": "3.0.2",
    "@themerdev/wallpaper-trianglify": "2.0.2",
    "@themerdev/xcode": "2.1.1",
    "@themerdev/xresources": "1.0.3",
    "themer": "3.3.9",
    "themer-gnome-terminal": "^1.0.1",
    "themer-i3": "^1.0.1"
  },
  "devDependencies": {
    "trim": ">=1.0.1"
  },
  "scripts": {
    "build:theme": "bash ./bin/themer.sh",
    "release": "standard-version --release-as patch -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" && git push --follow-tags origin main",
    "release:major": "standard-version --release-as major -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" && git push --follow-tags origin main",
    "release:minor": "standard-version --release-as minor -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" && git push --follow-tags origin main"
  },
  "main": "index.js"
}
